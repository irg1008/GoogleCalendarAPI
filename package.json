	"license": "ISC",
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@types/module-alias": "^2.0.1",
		"@types/node": "16.9.6",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"jest": "^27.2.1",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"tsc-alias": "^1.3.9",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"axios": "^0.21.4"
	}
